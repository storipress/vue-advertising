{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:js-lib"],
  "packageRules": [
    {
      "matchDepTypes": ["devDependencies"],
      "matchUpdateTypes": [
        "minor",
        "patch",
        "pin",
        "lockFileMaintenance",
        "digest"
      ],
      "automerge": true
    }
  ],
  "postUpdateOptions": ["yarnDedupeHighest"]
}
